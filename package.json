{
  "name": "vanilla",
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Edge versions"
  ],
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html ./"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "excludeGlob": "**/!(assets)/*.+(!(json))"
      }
    ],
    "watcherGlob": "**"
  },
  "keywords": [
    "fkg",
    "dmm"
  ],
  "author": "https://github.com/Amarillys",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel": "^2.4.0",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.5.0"
  },
  "dependencies": {
    "@pixi-spine/all-3.8": "^3.0.13",
    "crypto-js": "4.0.0",
    "file-saver": "2.0.2",
    "g": "^2.0.1",
    "jszip": "3.5.0",
    "md5": "^2.3.0",
    "pako": "^2.0.4",
    "pixi-spine": "2.1.10",
    "pixi.js": "5.3.3"
  }
}
